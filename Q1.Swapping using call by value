#include<iostream>
using namespace std;
void swap(int x,int y){
    int temp;
    temp=x;
    x=y;
    y=temp;
    return;
}
int main(){
    int a=5,b=10;
    cout<<"CALL BY VALUE "<<endl;
    cout<<"before swapping :"<<endl<<"a = "<<a<<endl<<"b = "<<b<<endl;
    swap(a,b);
    cout<<"after swapping "<<endl;;
    cout<<"a = "<<a<<endl<<"b = "<<b;
    
}

OUTPUT:
CALL BY VALUE 
before swapping :
a = 5
b = 10
after swapping 
a = 5
b = 10
